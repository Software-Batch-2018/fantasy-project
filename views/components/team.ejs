<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Fantasy</title>
</head>
<link rel="stylesheet" href="/css/fantasy.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css">
<link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Open+Sans'>


<body>
    <%- include('./header') %>
    <section class="main-body">

        <div id="info" class="info">
        </div>

        <div class="formation">
            <div class="pitch">
                <div class="top-box"></div>
                <div class="top-inner-box"></div>
                <div class="centre-circle"></div>
                <div class="centre-line"></div>
                <div class="bottom-box"></div>
                <div class="bottom-inner-box"></div>
                <div class="player-card">
                    <div class="player-card-section">
                        <div id="forward" class="FW">
                            <% FW.forEach(function(rank){ %> 
                                <div class="player" id="GK"><div class="select-card"><img class="card-image" src="<%= rank.Player_image %> " ><br><%= rank.Player_name %></div></div>
                            <% }) %>

                        </div>
                    </div>
                    <div class="player-card-section">
                        <div id="mid" class="MF">
                            <% MF.forEach(function(rank){ %> 
                                <div class="player" id="GK"><div class="select-card"><img class="card-image" src="<%= rank.Player_image %> " ><br><%= rank.Player_name %></div></div>
                            <% }) %>
                        </div>
                    </div>
                    <div class="player-card-section">
                        <div id="def" class="DF">
                            <% DF.forEach(function(rank){ %> 
                                <div class="player" id="GK"><div class="select-card"><img class="card-image" src="<%= rank.Player_image %> " ><br><%= rank.Player_name %></div></div>
                            <% }) %>
                        </div>
                    </div>
                    <div class="player-card-section">
                        <div id="goal" class="GK">
                            <% GK.forEach(function(rank){ %> 
                                <div class="player" id="GK"><div class="select-card"><img class="card-image" src="<%= rank.Player_image %> " ><br><%= rank.Player_name %></div></div>
                            <% }) %>
                        </div>
                        <div class="popup">
                            <span class="popuptext" id="myPopup"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div>
        </div>
    </section>
</div>
</div>


</body>
</html>